{
  "name": "remoteable",
  "private": true,
  "version": "1.0.1",
  "description": "remote jobs aggregator",
  "jest": {
    "setupFiles": [
      "./app/scripts/testSetup",
      "jest-localstorage-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(vue-switches)/)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "mapCoverage": true
  },
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:firefox": "gulp --production --vendor=firefox && gulp pack --vendor=firefox",
    "build:opera": "gulp --production --vendor=opera && gulp pack --vendor=opera",
    "dev": "node-sass app/component/switches.scss app/styles/switches.css && npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "test": "jest"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "axios": "^0.17.1",
    "babel-cli": "6.x.x",
    "babel-core": "^6.26.3",
    "babel-eslint": "6.x.x",
    "babel-jest": "^22.4.4",
    "babel-loader": "6.x.x",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es3": "^1.0.1",
    "babel-preset-stage-0": "6.x.x",
    "babel-register": "^6.26.0",
    "bulma": "^0.3.1",
    "chai": "3.x.x",
    "chrome-mock": "0.0.9",
    "chromereload": "0.x.x",
    "css-loader": "^0.26.4",
    "debounce": "1.x.x",
    "del": "2.x.x",
    "eslint": "3.x.x",
    "eslint-loader": "1.x.x",
    "file-loader": "^1.1.11",
